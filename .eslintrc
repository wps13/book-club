{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["react", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", "jsx"]
      }
    ],
    "prettier/prettier": "error",
    "react/prop-types": 0,
    "no-undef": "warn",
    "max-lines": "warn",
    "max-len": "off"
  },
  "env": {
    "browser": true
  }
}
